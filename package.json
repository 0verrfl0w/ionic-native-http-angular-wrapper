    "angular/common/http",
    "cors",
    "wkwebview",
    "ionic-native"
  ],
  "author": "Ganey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ganey/ionic-native-http-angular-wrapper/issues"
  },
  "homepage": "https://github.com/ganey/ionic-native-http-angular-wrapper#readme",
  "devDependencies": {
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@ionic-native/http": "^5.3.0",
    "@types/node": "^6.0.60",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
