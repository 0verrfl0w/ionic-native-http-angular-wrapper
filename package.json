{
  "name": "ionic-native-http-angular-wrapper",
  "version": "1.0.2",
  "description": "A basic package for making @ionic-native/http behave with observables and fall back to @angular/common/http.",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": "dist",
  "scripts": {
    "ngc": "ngc",
    "build": "rm -rf aot dist && npm run ngc",
